                  <TableColumn prefWidth="75.0" text="Screen Size" />
                  <TableColumn prefWidth="75.0" text="Memory" />
                  <TableColumn prefWidth="75.0" text="Front Cam" />
                  <TableColumn prefWidth="75.0" text="Rear Cam" />
              </columns>
            </TableView>
            <Button mnemonicParsing="false" text="Create New Phone Object" />
